{
  "$schema": "https://opencode.ai/config.json",
  "provider": {
    "glm-air": {
      "npm": "@ai-sdk/openai-compatible",
      "name": "glm-air",
      "options": {
        "baseURL": "https://llm.hnatekmar.dev/glm-air/v1"
      },
      "models": {
        "glm-air": {
          "name": "glm-air",
          "tool_call": true
        }
      }
    },
    "qwen-next-instruct": {
      "npm": "@ai-sdk/openai-compatible",
      "name": "qwen-next-instruct",
      "options": {
        "baseURL": "https://llm.hnatekmar.dev/qwen-next-instruct/v1"
      },
      "models": {
        "qwen-next-instruct": {
          "name": "qwen-next-instruct",
          "tool_call": true,
          "reasoning": false,
          "options": {
            "temperature": 0.7,
            "top_k": 20,
            "top_p": 0.8,
            "min_p": 0,
            "presence_penalty": 1
          },
        }
      }
    },
    "qwen-next-thinking": {
      "npm": "@ai-sdk/openai-compatible",
      "name": "qwen-next-thinking",
      "options": {
        "baseURL": "https://llm.hnatekmar.dev/qwen-next-thinking/v1"
      },
      "models": {
        "qwen-next-thinking": {
          "name": "qwen-next-thinking",
          "tool_call": true,
          "reasoning": true,
          "options": {
            "temperature": 0.8,
            "top_k": 20,
            "top_p": 0.8,
            "min_p": 0,
            "presence_penalty": 1
          }
        }
      }
    }
  }
}
